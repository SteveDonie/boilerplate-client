{
  "name": "client_boilerplate",
  "version": "0.0.1",
  "description": "isomorphic web app boilerplate (Node.js, Webpack, Babel, ESLint, Flow, React.js, Redux.js, SAAS)",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "clean": "rm -rf app/dist app/build",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "plugins": [
      "transform-react-inline-elements"
    ]
  },
  "author": "russell sherman",
  "license": "CC-BY-SA-4.0",
  "repository": "https://github.com/BurnerDev/boilerplate-client.git",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "react-addons-test-utils": "^15.1.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-middleware": "^1.5.1"
  },
  "dependencies": {
    "bootstrap-social": "^5.0.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.0.0",
    "node-sass": "^3.8.0",
    "nprogress": "^0.2.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.28.5",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-modal": "^1.3.0",
    "react-router": "^2.4.1",
    "recharts": "^0.11.0",
    "sass-loader": "^3.2.1",
    "startbootstrap-sb-admin-2": "^1.0.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  }
}
